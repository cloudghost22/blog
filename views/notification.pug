-var successTemp = {success}
-var errorTemp = {error}

div(class='ui grid')
    div(class='four wide column')
    div(class='eight wide column')
        if successTemp
            div(class='ui success message')
                p #{success}
        else if errorTemp
            div(class='ui error message')
                p #{error}

